<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
		<section name="robot-settings" type="Supplier2Presta.Service.Config.RobotSettingsSection, Supplier2Presta.Service"/>
	</configSections>
	<appSettings>
		<add key="LOGENTRIES_TOKEN" value=""/>
		<add key="LOGENTRIES_ACCOUNT_KEY" value=""/>
		<add key="LOGENTRIES_LOCATION" value=""/>
		<add key="api-url" value="https://shop.com/api"/>
		<add key="api-access-token" value=""/>
	</appSettings>
	<robot-settings>
		<suppliers>
			<supplier name="happiness_stock" supplier="Happiness" type="Stock" url="http://supplier.com/stockprice.xml" archive-dir="archive" encoding="utf-8">
				<multiplicators default="1.3">
					<rule name="low-price" min-price="0" max-price="499.99" value="1.4"/>
					<rule name="mid-price" min-price="500" max-price="1499.99" value="1.3"/>
					<rule name="mid-high-price1" min-price="1500" max-price="1999.99" value="1.3"/>
					<rule name="mid-high-price2" min-price="2000" max-price="2999.99" value="1.3"/>
					<rule name="mid-high-price3" min-price="3000" max-price="3499.99" value="1.3"/>
					<rule name="mid-high-price4" min-price="3500" max-price="3999.99" value="1.25"/>
					<rule name="high-price" min-price="4000" max-price="9999" value="1.2"/>
					<rule name="very-high-price" min-price="10000" max-price="1000000" value="1.15"/>
				</multiplicators>
			</supplier>
			<supplier name="happiness_full" supplier="Happiness" type="Full" url="http://supplier.com/fullprice.xml" archive-dir="archive\full" encoding="utf-8" format-file="happiness.xml">
				<multiplicators default="1.4">
					<rule name="low-price" min-price="0" max-price="499.99" value="1.4"/>
					<rule name="mid-price" min-price="500" max-price="1499.99" value="1.3"/>
					<rule name="mid-high-price1" min-price="1500" max-price="1999.99" value="1.3"/>
					<rule name="mid-high-price2" min-price="2000" max-price="2999.99" value="1.3"/>
					<rule name="mid-high-price3" min-price="3000" max-price="3499.99" value="1.3"/>
					<rule name="mid-high-price4" min-price="3500" max-price="3999.99" value="1.25"/>
					<rule name="high-price" min-price="4000" max-price="9999" value="1.2"/>
					<rule name="very-high-price" min-price="10000" max-price="1000000" value="1.15"/>
				</multiplicators>
			</supplier>
			<supplier name="happiness_supersale" supplier="Happiness" type="Discount" url="http://supplier.com/saleprice.csv" archive-dir="archive\discount" encoding="utf-8" format-file="happiness_sale.xml" discount="50">
				<multiplicators default="0">
				</multiplicators>
			</supplier>
		</suppliers>
